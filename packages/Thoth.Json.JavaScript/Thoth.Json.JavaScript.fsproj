<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Description>
            Elm-inspired encoder and decoder for JSON, this package is for JavaScript runtime.
        </Description>
        <FablePackageType>library</FablePackageType>
        <PackageTags>fable-javascript;fsharp;json</PackageTags>
    </PropertyGroup>
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="Decode.fs" />
        <Compile Include="Encode.fs" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Fable.Core" />
        <PackageReference Include="Fable.Package.SDK" />
        <PackageReference Include="Ionide.KeepAChangelog.Tasks" PrivateAssets="all" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\Thoth.Json.Core\Thoth.Json.Core.fsproj" />
    </ItemGroup>
</Project>
